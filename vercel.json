{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/videos/:path*",
      "destination": "/api"
    },
    {
      "source": "/api/:path*",
      "destination": "/api"
    }
  ],
  "routes": [
    {
      "src": "/cms/(.*)",
      "dest": "/cms/$1"
    },
    {
      "src": "/(.*\\.(js|css|jpg|png|svg|ico))",
      "dest": "/public/$1"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    }
  ]
}
